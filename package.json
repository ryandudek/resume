{
  "name": "ryandudek",
  "version": "6.6.6",
  "description": "Ryan Dudek - Keeping it Awesome",
  "devDependencies": {
    "@11ty/eleventy": "0.8.3",
    "clean-css-cli": "4.3.0",
    "concat": "1.0.3",
    "imagemin-cli": "5.0.0",
    "markdown-it": "9.1.0",
    "markdown-it-anchor": "5.2.4",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "onchange": "6.0.0",
    "uglify-js": "3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ryandudek/ryandudek.github.io.git"
  },
  "author": "Ryan Dudek",
  "bugs": {
    "url": "https://github.com/ryandudek/ryandudek.github.io/issues"
  },
  "homepage": "https://github.com/ryandudek/ryandudek.github.io#readme",
  "dependencies": {},
  "scripts": {
    "build": "npm run build:css && npm run build:js && npm run build:img",
    "build:css": "npm run scss && npm run concat:css && npm run cssmin",
    "build:img": "npm run imagemin",
    "build:js": "npm run concat:js && npm run uglify",
    "concat": "npm run concat:css && npm run concat:js",
    "concat:css": "concat -o dist/css/styles.css dist/css/style.css assets/css/test.css",
    "concat:js": "mkdir -p dist/js && concat -o dist/js/scripts.js assets/js/test1.js assets/js/test2.js",
    "cssmin": "cleancss -o dist/css/styles.min.css dist/css/styles.css",
    "imagemin": "imagemin --out-dir=dist/img assets/img/**/*.{png,jpg,gif}",
    "scss": "node-sass -o dist/css src/styles/style.scss",
    "uglify": "uglifyjs -o dist/js/scripts.min.js dist/js/scripts.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "onchange 'src/styles/*.scss' -- npm run build:css",
    "watch:js": "onchange 'src/js/*.js' -- npm run build:js"
  },
  "keywords": [],
  "license": "ISC"
}
