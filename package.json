{
  "name": "ryandudek",
  "version": "6.6.6",
  "description": "Ryan Dudek - Keeping it Awesome",
  "devDependencies": {
    "@11ty/eleventy": "0.8.3",
    "clean-css-cli": "4.3.0",
    "imagemin-cli": "5.0.0",
    "markdown-it": "9.1.0",
    "markdown-it-anchor": "5.2.4",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "nunjucks": "3.2.0",
    "onchange": "6.0.0",
    "uglify-js": "3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ryandudek/ryandudek.github.io.git"
  },
  "author": "Ryan Dudek",
  "bugs": {
    "url": "https://github.com/ryandudek/ryandudek.github.io/issues"
  },
  "homepage": "https://github.com/ryandudek/ryandudek.github.io#readme",
  "dependencies": {
    "inline-svg": "^2.2.3"
  },
  "scripts": {
    "build": "npm run build:css && npm run build:img && eleventy --serve",
    "build:css": "npm run scss && npm run cssmin",
    "build:img": "npm run imagemin",
    "cssmin": "cleancss -o dist/css/styles.min.css dist/css/style.css",
    "imagemin": "imagemin --out-dir=dist/img src/assets/img/**/*.{png,jpg,gif}",
    "scss": "node-sass -o dist/css src/styles/style.scss",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "onchange 'src/styles/*.scss' -- npm run build:css"
  },
  "keywords": [],
  "license": "ISC"
}
